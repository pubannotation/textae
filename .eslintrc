{
  "rules": {
    "no-unused-vars": "off",
    "one-var": ["warn", "never"],
    "no-var": "warn",
    "prefer-const": "warn",
    "object-shorthand": ["warn", "always"],
    "prefer-arrow-callback": "warn",
    "prefer-template": "warn"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
    "jsPlumb": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion":  2017
  } 
}
